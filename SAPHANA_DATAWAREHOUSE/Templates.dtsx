cast(OITM.ItmsGrpCod as int) ItmsGrpCod ,  
OITW.ItemCode, 
OITM.U_Item_Visibility, 
OITM.U_SUB_GR, 
OITM.ValidFor, 
OITM.ValidFrom, 
OITM.ValidTo, 
OITM.CountryCode, 
cast(OITM.EvalSystem as nvarchar(10)) as CostFlag,
case when OITM.EvalSystem = 'S' then OITW.AvgPrice else Null end as StandardCost,
cast(OITM.U_ItemCode as varchar(50)) as SapItemReferenceCode,
case when OITM.countrycode = 'BI' then OITM.ItemName else OITM.FrgnName end ForeignName, 
case
when OITM.ValidFor = 'Y' and (OITM.ValidTo is null or  OITM.validto &gt;= getdate()) and (OITM.validfrom is null or OITM.validfrom &lt;= cast(getdate() as date)) then cast( 1 as bit)
else cast(0 as bit) end as Active,
OITB.PrcrmntMtd,
OITB.Locked, 
OITM.Countryid, 
case when OITM.SellItem = 'Y' then 1 else 0 end as IsSaleItem,
case when OITM.PrchseItem = 'Y' then 1 else 0 end as IsPurchaseItem ,
case when OITM.QryGroup1 = 'Y' then 1 else 0 end  as IsInvoiceItem,
case when OITM.InvntItem = 'Y' then 1 else 0 end  as IsInventoryItem, 
OITM.BuyUnitMsr as PurchaseUnitOfMeasure,
OITM.InvntryUom as InventoryUnitOfMeasure,
OITM.SalUnitMsr as SaleUnitOfMeasure
FROM   OITW_HANA OITW
left join OITM_HANA OITM on OITW.itemcode = OITM.itemcode and OITM.CountryCode = OITW.CountryCode
left join OITB_HANA OITB on OITB.ItmsGrpCod = OITM.ItmsGrpCod and OITB.CountryCode = OITM.CountryCode
left join OWHS_HANA owhs on OWHS.whscode = oitw.whscode and OWHS.CountryCode = OITW.CountryCode
WHERE OITM.CountryCode in ('KE','RW', 'TZ', 'BI', 'MW', 'UG','ZA', 'NG', 'RW-TBR') ),

 b as (
SELECT distinct 
case when OITM.countrycode = 'BI' then OITM.FrgnName else OITM.ItemName end ItemName, 
OITM.ItmsGrpCod,  
OITW.ItemCode, 
OITM.U_Item_Visibility, 
